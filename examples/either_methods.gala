package main

import "fmt"

func main() {
    val e = Left[int, string](42)

    val msg1 = e match {
        case Left(v) => fmt.Sprintf("It is Left, Value: %v", v)
        case _ => "Not Left"
    }
    fmt.Println(msg1)

    val e2 = Right[int, string]("hello")
    val msg2 = e2 match {
        case Right(v) => fmt.Sprintf("It is Right, Value2: %v", v)
        case _ => "Not Right"
    }
    fmt.Println(msg2)
}
