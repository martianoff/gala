package main

import "fmt"

func negate(x int) int = -x

func main() {
    // Unary minus on literal
    val a = -42
    fmt.Println(a)

    // Unary minus on float literal
    val b = -3.14
    fmt.Println(b)

    // Unary minus on variable
    val x = 10
    val c = -x
    fmt.Println(c)

    // Unary minus in function argument
    fmt.Println(negate(-5))

    // Unary minus in expression
    val d = -x + 20
    fmt.Println(d)

    // Unary NOT
    val e = !true
    fmt.Println(e)
}
